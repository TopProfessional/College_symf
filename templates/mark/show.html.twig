{% block title %}Mark{% endblock %}

{% block body %}
    <h1>Mark</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Mark</th>
                <td>{{ mark.mark }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ mark.date ? mark.date|date('Y-m-d') : '' }}</td>
